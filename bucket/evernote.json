    "url": "https://cdn1.evernote.com/boron/win/builds/Evernote-10.62.2-win-ddl-stage-20230919180646-84aea9b18037f8e52dce4809d0f844e94d428a50-setup.exe#/dl.7z",
    "hash": "sha512:b1b73c1e18b9d427b8722e3cd25166352e64e5a30678f56ff7a39a24376832da8d1682a9cac64bc8d35c5c41c3aac3cce09280d6b2c215e32818e845cf6531fd",
    "installer": {
        "script": [
            "Remove-Item \"$dir\\Unins*\", \"$dir\\resources\" -Force -Recurse",
            "if ($architecture -eq '64bit') {",
            "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
            "} elseif ($architecture -eq '32bit') {",
            "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-32.7z\" \"$dir\"",
            "}",
            "Remove-Item \"$dir\\`$PLUGINSDIR\" -Force -Recurse"
        ]
    },
    "shortcuts": [
        [
            "Evernote.exe",
            "Evernote"
        ]
    ],
    "checkver": {
        "url": "https://evernote.s3.amazonaws.com/boron/win/public/latest.yml",
        "regex": "Evernote-([\\d.]+)-(?<tail>.+).exe"
    },
    "autoupdate": {
        "url": "https://cdn1.evernote.com/boron/win/builds/Evernote-$version-$matchTail.exe#/dl.7z",
        "hash": {
            "url": "https://evernote.s3.amazonaws.com/boron/win/public/latest.yml",
            "regex": "sha512: $base64"
        }
    }
}
